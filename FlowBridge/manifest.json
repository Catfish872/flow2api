{
  "manifest_version": 2,
  "name": "Flow 2API Force Bridge",
  "version": "1.3",
  "permissions": [
    "https://labs.google/*",
    "http://127.0.0.1/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": ["https://labs.google/fx/tools/flow/project/*"],
      "js": ["bridge.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}